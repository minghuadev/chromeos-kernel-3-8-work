/dts-v1/;
#include "exynos5420.dtsi"
#include "exynos5420-peach-pit.dtsi"

/ {
	model = "Google Peach Pit Rev 3";

	compatible = "google,pit-rev3", "google,pit",
		"google,peach", "samsung,exynos5420";

	/* Avoid suspend/resume problems on non-screened silicon */
	hsi2c@12CA0000 {
		max77802-pmic@9 {
			voltage-regulators {
				buck3_reg: BUCK3 {
					regulator-op-mode = <2>;
				};
			};
		};
	};
};
